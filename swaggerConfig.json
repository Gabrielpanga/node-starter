{
  "swagger": {
    "outputDirectory": "./",
    "entryFile": "./src/controllers/*.ts",
    "outputFormat": "openapi_3",
    "host": "localhost:9090",
    "version": "1.0",
    "name": "Node & GraphQL base starter",
    "description": "Node & GraphQL base starter - description",
    "license": "MIT",
    "basePath": "/",
    "securityDefinitions": {
      "api_key": {
        "type": "apiKey",
        "name": "access_token",
        "in": "query"
      }
    },
    "spec": {
      "schemes": ["http"]
    }
  }
}
